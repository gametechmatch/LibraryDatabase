/*CREATE ADDITIONAL TABLES*/
/*Create Creator Table*/
CREATE TABLE Creator (
CreatorID BIGINT NOT NULL IDENTITY,
CreatorFirstName NVARCHAR(70) NOT NULL,
CreatorLastName NVARCHAR(70) NOT NULL,
CreatorDOB DATE NOT NULL,
GenderID NCHAR(7) NOT NULL,
CountryID NCHAR(8) NOT NULL,
PRIMARY KEY(CreatorID),
CONSTRAINT FK_C_GID FOREIGN KEY(GenderID) REFERENCES "Gender"(GenderID),
CONSTRAINT FK_C_CID FOREIGN KEY(CountryID) REFERENCES "Country"(CountryID),
);

/*Create User table*/
CREATE TABLE "User"(
UserID BIGINT NOT NULL IDENTITY,
UserPassword NVARCHAR(30) NOT NULL,
SystemPermissionID NCHAR(14) NOT NULL DEFAULT 'PERMISSION-001',
UserFirstName NVARCHAR(70) NOT NULL,
UserLastName NVARCHAR(70) NOT NULL,
UserDOB DATE NOT NULL,
UserEmail NVARCHAR(100) NOT NULL,
UserEmailTwo NVARCHAR(100) NULL,
UserPhone NCHAR(10) NOT NULL,
UserPhoneTwo NCHAR(10) NULL,
UserStreetAddress NVARCHAR(150) NOT NULL,
UserStreetAddressTwo NVARCHAR(150) NULL,
UserAptNo NVARCHAR(50) NULL,
UserCity NVARCHAR(100) NOT NULL,
StateID NCHAR(5) NOT NULL DEFAULT 'ST-WI',
UserZipCode INT NOT NULL,
UserStartDateTime DATETIME NOT NULL,
UserEndDateTime DATETIME NULL,
UserIsStaff BIT NOT NULL DEFAULT 0,
GenderID NCHAR(7) NOT NULL,
PRIMARY KEY(UserID),
CONSTRAINT FK_U_SPID FOREIGN KEY(SystemPermissionID) REFERENCES SystemPermission(SystemPermissionID),
CONSTRAINT FK_U_SID FOREIGN KEY(StateID) REFERENCES "State"(StateID),
CONSTRAINT FK_U_GID FOREIGN KEY(GenderID) REFERENCES "Gender"(GenderID),
CHECK (UserZipCode >= 0 AND UserZipCode <= 99999),
CHECK (UserIsStaff = 0 OR UserIsStaff = 1),
);

/*Create Schedule table*/
CREATE TABLE Schedule(
ScheduleID BIGINT NOT NULL IDENTITY,
UserID BIGINT NOT NULL,
ScheduleDate DATE NOT NULL,
ScheduleStartTime TIME NOT NULL,
ScheduleEndTime TIME NOT NULL,
ScheduleOvertimeMinutes SMALLINT NOT NULL DEFAULT 0,
ScheduleWeekend BIT NOT NULL,
ScheduleHoliday BIT NOT NULL DEFAULT 0,
ScheduleHourlyPay SMALLMONEY NOT NULL,
DepartmentID NCHAR(8) NOT NULL,
PRIMARY KEY(ScheduleID),
CONSTRAINT FK_S_UID FOREIGN KEY(UserID) REFERENCES "User"(UserID),
CONSTRAINT FK_S_DID FOREIGN KEY(DepartmentID) REFERENCES Department(DepartmentID),
CHECK (ScheduleOvertimeMinutes >= 0 AND ScheduleOvertimeMinutes <= 1440),
CHECK (ScheduleHourlyPay >= 7.25 AND ScheduleHourlyPay <= 500.00),
);

/*Create Item Table*/
CREATE TABLE Item (
ItemID BIGINT NOT NULL IDENTITY,
ItemName NVARCHAR (150) NOT NULL,
PRIMARY KEY(ItemID),
);

/*Create Media Detail Table*/
CREATE TABLE MediaDetail (
MediaDetailID BIGINT NOT NULL IDENTITY,
ItemID BIGINT NOT NULL,
MediaTypeID NCHAR(9) NOT NULL,
MediaDetailISBN NVARCHAR(300) NULL,
MediaDetailDOI NVARCHAR(300) NULL,
MediaDetailPublisher NVARCHAR(50) NOT NULL,
MediaDetailYear SMALLINT NOT NULL,
MediaDetailGenre NVARCHAR(50) NOT NULL,
ContentRatingID NVARCHAR(50) NOT NULL,
MediaPurchaseCost SMALLMONEY NOT NULL,
PRIMARY KEY(MediaDetailID),
CONSTRAINT FK_MD_IID FOREIGN KEY(ItemID) REFERENCES Item(ItemID),
CONSTRAINT FK_MD_MTID FOREIGN KEY(MediaTypeID) REFERENCES MediaType(MediaTypeID),
CONSTRAINT FK_MD_CRID FOREIGN KEY(ContentRatingID) REFERENCES ContentRating(ContentRatingID),
);

/*Create Room Detail Table*/
CREATE TABLE RoomDetail(
RoomDetailID INT NOT NULL IDENTITY,
ItemID BIGINT NOT NULL,
RoomDetailRoomFloor TINYINT NOT NULL,
RoomDetailRoomNumber TINYINT NOT NULL,
RoomDetailTotalPhones TINYINT NOT NULL,
RoomDetailTotalComputers TINYINT NOT NULL,
RoomDetailTotalTables TINYINT NOT NULL,
RoomDetailTotalChairs SMALLINT NOT NULL,
RoomDetailMaxCapacity SMALLINT NOT NULL,
RoomDetailCostPerMinute SMALLMONEY NOT NULL,
RoomDetailMaxReservationMinutes SMALLINT NOT NULL,
PRIMARY KEY(RoomDetailID),
CONSTRAINT FK_RD_IID FOREIGN KEY(ItemID) REFERENCES Item(ItemID),
CHECK (RoomDetailRoomFloor >= 0 AND RoomDetailRoomFloor <=3),
CHECK (RoomDetailRoomNumber >= 0 AND RoomDetailRoomNumber <=900),
CHECK (RoomDetailTotalPhones >= 0 AND RoomDetailTotalPhones <=15),
CHECK (RoomDetailTotalComputers >= 0 AND RoomDetailTotalComputers <=50),
CHECK (RoomDetailTotalTables >= 0 AND RoomDetailTotalTables <=100),
CHECK (RoomDetailMaxCapacity >= 0 AND RoomDetailMaxCapacity <=2000),
CHECK (RoomDetailMaxReservationMinutes >= 0 AND RoomDetailMaxReservationMinutes <= 480),
);

/*Create Tech Detail Table*/
CREATE TABLE TechDetail(
TechDetailID BIGINT NOT NULL IDENTITY,
ItemID BIGINT NOT NULL,
TechTypeID INT NOT NULL,
TechDetailBrand NVARCHAR(60) NOT NULL,
TechDetailTotalHDMIPorts TINYINT NOT NULL,
TechDetailTotalVGAPorts TINYINT NOT NULL,
TechDetailTotalDisplayPortPorts TINYINT NOT NULL,
TechDetailBluetoothEnabled BIT NOT NULL,
TechDetailWirelessConnection BIT NOT NULL,
TechDetailPurchaseCost SMALLMONEY NOT NULL,
TechDetailCostPerMinute SMALLMONEY NOT NULL,
TechDetailMaxReservationMinutes SMALLINT NOT NULL,
PRIMARY KEY(TechDetailID),
CONSTRAINT FK_TD_IID FOREIGN KEY(ItemID) REFERENCES Item(ItemID),
CONSTRAINT FK_TD_TTID FOREIGN KEY(TechTypeID) REFERENCES TechType(TechTypeID),
CHECK (TechDetailTotalHDMIPorts >=0 AND TechDetailTotalHDMIPorts <= 30),
CHECK (TechDetailTotalVGAPorts >=0 AND TechDetailTotalVGAPorts <= 30),
CHECK (TechDetailTotalDisplayPortPorts >=0 AND TechDetailTotalDisplayPortPorts <= 30),
CHECK (TechDetailMaxReservationMinutes >= 0 AND TechDetailTotalDisplayPortPorts <= 480),
);

/*Create Service Detail Table*/
CREATE TABLE ServiceDetail(
ServiceDetailID INT NOT NULL IDENTITY,
ItemID BIGINT NOT NULL,
ServiceDetailCostPerService SMALLMONEY NOT NULL,
PRIMARY KEY(ServiceDetailID),
CONSTRAINT FK_SD_IID FOREIGN KEY(ItemID) REFERENCES Item(ItemID),
);

/*Create Receipt Table*/
CREATE TABLE Receipt (
ReceiptID BIGINT NOT NULL IDENTITY,
ReceiptDateTime DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP,
ReceiptTotalDue SMALLMONEY NULL,
UserID BIGINT NOT NULL,
PRIMARY KEY(ReceiptID),
CONSTRAINT FK_R_UID FOREIGN KEY(UserID) REFERENCES "User"(UserID),
);

/*Create Receipt Transaction Table*/
CREATE TABLE ReceiptTransaction (
ReceiptTransactionID BIGINT NOT NULL IDENTITY,
TransactionTypeID NCHAR(9) NOT NULL,
ItemID BIGINT NOT NULL,
ReceiptTransactionItemQuantity INT NOT NULL,
ReceiptTransactionTotalCharge SMALLMONEY NOT NULL,
ReceiptID BIGINT NOT NULL,
PRIMARY KEY(ReceiptTransactionID),
CONSTRAINT FK_RT_TTID FOREIGN KEY(TransactionTypeID) REFERENCES TransactionType(TransactionTypeID),
CONSTRAINT FK_RT_IID FOREIGN KEY(ItemID) REFERENCES Item(ItemID),
CONSTRAINT FK_RT_RID FOREIGN KEY(ReceiptID) REFERENCES Receipt(ReceiptID),
);

/*Create Reservation Detail Table*/
CREATE TABLE ReservationDetail (
ReservationDetailID BIGINT NOT NULL IDENTITY,
ReceiptTransactionID BIGINT NOT NULL,
ReservationDetailDate DATE NOT NULL,
ReservationDetailStartTime TIME(7) NOT NULL,
ReservationDetailEndTime TIME(7) NOT NULL,
PRIMARY KEY(ReservationDetailID),
CONSTRAINT FK_RSD_RTID FOREIGN KEY(ReceiptTransactionID) REFERENCES ReceiptTransaction(ReceiptTransactionID),
);

/*Create Loan Detail Table*/
CREATE TABLE LoanDetail (
LoanDetailID BIGINT NOT NULL IDENTITY,
ReceiptTransactionID BIGINT NOT NULL,
LoanTypeID NCHAR(12) NOT NULL,
LoanDetailDueDate DATE NOT NULL,
LoanDetailItemReturned BIT NOT NULL DEFAULT 0, 
PRIMARY KEY(LoanDetailID),
CONSTRAINT FK_LD_LTID FOREIGN KEY(LoanTypeID) REFERENCES LoanType(LoanTypeID),
CONSTRAINT FK_LD_RTID FOREIGN KEY(ReceiptTransactionID) REFERENCES ReceiptTransaction(ReceiptTransactionID),
);

/*CREATE LINKING TABLES*/
/*Create User Alert Table*/
CREATE TABLE UserAlert (
UserID BIGINT NOT NULL,
AlertID INT NOT NULL,
CONSTRAINT UserAlertIDName PRIMARY KEY(UserID,AlertID),
CONSTRAINT FK_UA_UID FOREIGN KEY(UserID) REFERENCES "User"(UserID),
CONSTRAINT FK_UA_AID FOREIGN KEY(AlertID) REFERENCES Alert(AlertID),
);

/*Create Media Creator Table*/
CREATE TABLE MediaCreator (
MediaDetailID BIGINT NOT NULL,
CreatorID BIGINT NOT NULL,
CONSTRAINT MediaCreatorIDName PRIMARY KEY(MediaDetailID,CreatorID),
CONSTRAINT FK_MC_MID FOREIGN KEY(MediaDetailID) REFERENCES MediaDetail(MediaDetailID),
CONSTRAINT FK_MC_CID FOREIGN KEY(CreatorID) REFERENCES Creator(CreatorID),
);

/*Create Media Band Table*/
CREATE TABLE MediaBand (
MediaDetailID BIGINT NOT NULL,
BandID BIGINT NOT NULL,
CONSTRAINT MediaBandIDName PRIMARY KEY(MediaDetailID,BandID),
CONSTRAINT FK_MB_MID FOREIGN KEY(MediaDetailID) REFERENCES MediaDetail(MediaDetailID),
CONSTRAINT FK_MB_CID FOREIGN KEY(BandID) REFERENCES Band(BandID),
);
