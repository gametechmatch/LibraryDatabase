/*TABLE OF CONTENTS*/
/*
DATABASE CODE
_______CREATE TABLES WITHOUT FOREIGN KEYS_______
* Create Gender Table
* Create Department Table
* Create State Table
* Create Country Table
* Create System Permission Table
* Create Alert Table
* Create Content Rating Table
* Create Media Type Table
* Create Transaction Type Table
* Create Band Table

_______INSERT DATA INTO TABLES WITHOUT FOREIGN KEYS_______
* Insert Data into Gender Lookup Table
* Insert Data into Department Lookup Table
* Insert Data into State Lookup Table
* Insert Data into Country Lookup Table
* Insert Data into SystemPermission Lookup Table
* Insert Data into Alert Table
* Insert Data into Content Rating Lookup Table
* Insert Data into Media Type Table
* Insert Data into Transaction Type Table
* Insert Data into Band Table

_______CREATE ADDITIONAL TABLES_______
* Create Creator Table
* Create User table
* Create Schedule table
* Create Item Table
* Create Service Table
* Create Media Table
* Create Room Table
* Create Tech Table
* Create Room Reservation Table
* Create Tech Reservation Table
* Create Receipt Table
* Create Receipt Transaction

_______CREATE LINKING TABLES_______
* Create User Alert Table
* Create Media Creator Table
* Create Media Band Table

_______INSERT DATA INTO ADDITIONAL TABLES_______
* Insert Data into Creator Table
* Insert Data into User table
* Insert Data into Schedule table
* Insert Data into Item Table
* Insert Data into Service Table
* Insert Data into Media Table
* Insert Data into Room Table
* Insert Data into Tech Table
* Insert Data into Room Reservation Table
* Insert Data into Tech Reservation Table
* Insert Data into Receipt Table
* Insert Data into Receipt Transaction

_______INSERT INTO LINKING TABLES_______
* Insert Data into User Alert Table
* Insert Data into Media Creator table
* Insert Data into Media Band table

ADDITIONAL CODE
_______DELETE DATA_______
* Delete Data from Linking Entities
* Delete Data from Additional Entities
* Delete Data from Entities Without Foreign Keys

_______DELETE TABLES_______
* Delete Linking Tables
* Delete Additional Tables
* Delete Tables Without Foreign Keys

_______PRINT DATA_______
* Print all data from linking tables
* Print all data from additional tables
* Print all data from tables without foreign keys

_______OTHER CODE_______
* Check if Table is Present
*/
/*DATABASE CODE*/
/*CREATE TABLES WITHOUT FOREIGN KEYS*/
/*Create Gender Table*/
CREATE TABLE Gender(
GenderID NCHAR(7) NOT NULL,
GenderDescription NVARCHAR(100) NOT NULL,
PRIMARY KEY(GenderID),
);

/*Create Department Table*/
CREATE TABLE Department (
DepartmentID NCHAR(8) NOT NULL,
DepartmentDescription NVARCHAR(50) NOT NULL,
PRIMARY KEY(DepartmentID),
);

/*Create State Table*/
CREATE TABLE State (
StateID NCHAR(5) NOT NULL,
StateDescription NVARCHAR(50) NOT NULL,
PRIMARY KEY(StateID),
);

/*Create Country Table*/
CREATE TABLE Country (
CountryID NCHAR(8) NOT NULL,
CountryDescription NVARCHAR(100) NOT NULL,
PRIMARY KEY(CountryID),
);

/*Create System Permission Table*/
CREATE TABLE SystemPermission(
SystemPermissionID NCHAR(14) NOT NULL,
SystemPermissionDescription NVARCHAR(50) NOT NULL,
PRIMARY KEY(SystemPermissionID),
);

/*Create Alert Table*/
CREATE TABLE Alert (
AlertID INT NOT NULL IDENTITY,
AlertDescription NVARCHAR(30) NOT NULL,
PRIMARY KEY(AlertID),
);

/*Create Content Rating Table*/
CREATE TABLE ContentRating (
ContentRatingID NVARCHAR(50) NOT NULL,
ContentRatingDescription NVARCHAR(200) NOT NULL,
ContentRatingMinAge TINYINT NOT NULL,
PRIMARY KEY(ContentRatingID),
CHECK (ContentRatingMinAge >= 0 AND ContentRatingMinAge <=100),
);

/*Create MediaType Table*/
CREATE TABLE MediaType(
MediaTypeID NCHAR(9) NOT NULL,
MediaTypeName NVARCHAR(50) NOT NULL,
MediaTypeDigital BIT NOT NULL,
MediaTypeAudio BIT NOT NULL,
PRIMARY KEY(MediaTypeID),
);

/*Create Transaction Type Table*/
CREATE TABLE TransactionType (
TransactionTypeID NCHAR(9) NOT NULL,
TransactionTypeDescription NVARCHAR(50) NOT NULL,
TransactionTypePaymentInvolved BIT NOT NULL,
TransactionTypeFee BIT NOT NULL,
TransactionTypeReservation BIT NOT NULL,
PRIMARY KEY(TransactionTypeID),
);

/*Create Loan Type Table*/
CREATE TABLE LoanType (
LoanTypeID NCHAR(12) NOT NULL, -- LOANTYPE-001
LoanTypeDescription NVARCHAR(50) NOT NULL,
LoanTypeTotalDaysPerLoan INT NOT NULL,
LoanTypeMaxNumOfLoanRenewals INT NOT NULL,
LoanTypeMaxLatePeriodDays INT NOT NULL,
LoanTypeDailyLateFee SMALLMONEY NOT NULL,
PRIMARY KEY(LoanTypeID),
CHECK (LoanTypeTotalDaysPerLoan >= 0 AND LoanTypeTotalDaysPerLoan <=30),
CHECK (LoanTypeMaxNumOfLoanRenewals >= 0 AND LoanTypeMaxNumOfLoanRenewals <=3),
CHECK (LoanTypeMaxLatePeriodDays >= 0 AND LoanTypeMaxLatePeriodDays <=30),
CHECK (LoanTypeDailyLateFee >= 0.00 AND LoanTypeDailyLateFee <= 5.00),
);

/*Create Band Table*/
CREATE TABLE Band(
BandID BIGINT NOT NULL IDENTITY,
BandName NVARCHAR(70) NOT NULL,
PRIMARY KEY(BandID),
);

/*Create Tech Type Table*/
CREATE TABLE TechType(
TechTypeID INT NOT NULL IDENTITY,
TechTypeDescription NVARCHAR(50) NOT NULL, 
PRIMARY KEY(TechTypeID),
);
